    "b35cbbb8-9c64-46ee-a132-f66d2d6d62c5": {
      "type": "image-with-text",
      "blocks": {
        "template--15881460514863__b35cbbb8-9c64-46ee-a132-f66d2d6d62c5-heading-1": {
