    "f1b02bbe-63e6-4e88-ae0a-df7bd7b70845": {
      "type": "image-with-text",
      "blocks": {
        "template--15881460514863__f1b02bbe-63e6-4e88-ae0a-df7bd7b70845-heading-1": {
